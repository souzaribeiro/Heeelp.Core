@using Heeelp.Core.Common;
<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="https://code.angularjs.org/1.4.3/angular.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0rc1/angular-route.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="~/Scripts/ngMask.min.js"></script>

    <!-- Angular Bootstrap -->
    <script src="~/Scripts/angular-ui/ui-bootstrap.min.js"></script>
    <script src="~/Scripts/angular-ui/ui-bootstrap-tpls.min.js"></script>

    <link rel="stylesheet" type="text/css" href="/Content/css/admin-b.min.css">
    <link rel="stylesheet" type="text/css" href="/Content/css/admin-a.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/xeditable.min.css" />
    <!-- toastr -->
    <link href="~/Content/css/angular-toastr.min.css" rel="stylesheet" />


    <!-- Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,500,700' rel='stylesheet' type='text/css'>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">
    <!-- AngularAnimate -->
    <link href="~/Content/css/animate.css" rel="stylesheet" />




    @*html editor*@
    <script src="~/Content/js/angular-sanitize.min.js"></script>
    <script src="~/Content/js/textAngular.js"></script>

    

    

  
    @Scripts.Render("~/bundles/modernizr")

    @*<script src="/Scripts/vendor/jquery.ui.widget.js"></script>*@
    @*<link rel="stylesheet" href="/Content/jquery.fileupload.css">
        <script src="/Scripts/UploadFile/jquery.fileupload.js"></script>
        <script src="/Scripts/UploadFile/jquery.fileupload-angular.js"></script>*@
    <script src="~/Scripts/bs-table.js"></script>
    <script src="~/Scripts/bs-table.min.js"></script>
    <script src="~/Scripts/angular-toastr.tpls.min.js"></script>

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>


 

    @*<script src="/Scripts/UploadFile/jquery.fileupload-image.js"></script>*@
    <script src="~/app/Utils.js"></script>
    <script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="~/signalr/hubs"></script>
    <!--SignalR script to update the chat page and send messages.-->
    <script>
        var NotificationUrl = '@CustomConfiguration.WebApiNotification';
        var CoreUrl = '@CustomConfiguration.WebApiCore';
        var PromotionUrl = '@CustomConfiguration.WebApiPromotion';
        var UserSessionName = '@CustomConfiguration.UserSessionName';
        var MarketingUrl = '@CustomConfiguration.WebApiMarketing';
        var user_session = '@CustomConfiguration.UserSessionName';
        var ClassifiedUrl = '@CustomConfiguration.WebApiClassified';
        var FileServerUrl = '@CustomConfiguration.WebApiFileServer';

    </script>

    <script src="~/app/app.js"></script>
    <script src="~/Scripts/angular-locale_pt-br.js"></script>

</head>
<body>

    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="../" class="navbar-brand"><img src="http://heeelpfileserver.blob.core.windows.net/imagestheme/logoHeelpBlack.png" width="80" alt="Alternate Text" /></a>
                <span class="label label-default label-facilitador">Facilitador</span>
                <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse" id="navbar-main">
                <ul class="nav navbar-nav">

                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#" id="download">Adicionar <span class="caret"></span></a>
                        <ul class="dropdown-menu" aria-labelledby="download">
                            <li><a data-toggle="collapse" data-target="#navbar-main" href="/Expertise/ExpertiseAdd">Área de Atuação</a></li>
                            <li><a data-toggle="collapse" data-target="#navbar-main" href="/Person/PersonAdd">Empresa</a></li>
                            <li><a data-toggle="collapse" data-target="#navbar-main" href="/User/UserAdd">Usuário</a></li>
                            @*<li><a href="/Person/PersonAddressAdd">PersonAddress Add</a></li>*@
                            
                        </ul>
                    </li>
                    <li><a data-toggle="collapse" data-target="#navbar-main" href="/Notification/CommunicationTypeAdd">Mensagem</a></li>
                    <li><a data-toggle="collapse" data-target="#navbar-main" href="/Notification/NotificationAdd">Notificações</a></li>
                    <li><a data-toggle="collapse" data-target="#navbar-main" href="/Promotion/PromotionManagement">Promoções</a></li>
                    <li><a data-toggle="collapse" data-target="#navbar-main" href="/Promotion/ApprovePromotion">Aprovar Promoções</a></li>
                  
                  
                 
                    
                </ul>

                <ul class="nav navbar-nav navbar-right">
                    <li><a href="#" onclick="Logout()" class="logout" style="color:#e51c23">Sair</a></li>
                </ul>
              
            </div>
        </div>
    </div>

  

    <div class="page-container">
        @RenderBody()
    </div>


    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>
</html>
